{
  "extends": "../../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs"
, "postcss.config.cjs"  ],
  "compilerOptions": {
  //  "preserveSymlinks": true,
    "types": ["babylonjs"],
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "esnext.asynciterable",
      "DOM",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    //"esModuleInterop": true,
    "module": "esnext",
    // "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
