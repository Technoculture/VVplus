generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model asset {
  asset_id     Bytes   @id @db.Binary(16)
  name         String  @db.VarChar(255)
  description  String? @db.VarChar(255)
  category_id  Bytes   @unique(map: "category_idx") @db.Binary(16)
  three_d_url  String? @db.VarChar(255)
  thumbnail_sm String  @db.VarChar(255)
  hero         String  @db.VarChar(255)
}

model assetcategory {
  category_id Bytes   @id @db.Binary(16)
  name        String? @db.VarChar(255)
  description String  @db.VarChar(255)
}

model model {
  model_id     Bytes @id @db.Binary(16)
  user_id      Bytes @db.Binary(16)
  main_gate    Bytes @db.Binary(16)
  boundary     Bytes @db.Binary(16)
  balcony      Bytes @db.Binary(16)
  structure_gf Bytes @db.Binary(16)
  structure_ff Bytes @db.Binary(16)
  structure_sf Bytes @db.Binary(16)

  @@index([balcony], map: "balcony_idx")
  @@index([boundary], map: "boundary_idx")
  @@index([main_gate], map: "main_gate_idx")
  @@index([structure_ff], map: "structure_ff_idx")
  @@index([structure_gf], map: "structure_gf_idx")
  @@index([structure_sf], map: "structure_sf_idx")
  @@index([user_id], map: "user_idx")
}

model template {
  template_id Bytes  @id @db.Binary(16)
  model_id    Bytes  @unique(map: "Template_model_id_key") @db.Binary(16)
  name        String @db.VarChar(255)
}

model user {
  user_id      Bytes  @id @db.Binary(16)
  name         String @db.VarChar(255)
  phone_number String @db.VarChar(255)
  email_id     String @db.VarChar(255)
}
