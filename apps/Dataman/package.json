    "babel-plugin-import": "^1.13.5",
    "expo": "^47.0.0",
    "expo-dev-client": "~2.0.0",
    "expo-updates": "~0.15.4",
    "nativewind": "2.0.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.41.5",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
